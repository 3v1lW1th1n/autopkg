<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Input</key>
	<dict>
		<key>MUNKI_CATALOG</key>
		<string>common</string>
		<key>MUNKI_DESCRIPTION</key>
		<string>The Unarchiver is a much more capable replacement for &quot;Archive Utility.app&quot;, the built-in archive unpacker program on Mac OS X.</string>
		<key>MUNKI_DISPLAYNAME</key>
		<string>The Unarchiver</string>
		<key>NAME</key>
		<string>TheUnarchiver</string>
	</dict>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict/>
			<key>Processor</key>
			<string>TheUnarchiverURLProvider</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>download_dir</key>
				<string>%RECIPE_DIR%</string>
				<key>filename</key>
				<string>%NAME%.zip</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>archive_path</key>
				<string>%pathname%</string>
				<key>destination_path</key>
				<string>%RECIPE_DIR%/dmg_root</string>
				<key>purge_destination</key>
				<true/>
			</dict>
			<key>Processor</key>
			<string>Unzipper</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>dmg_root</key>
				<string>%RECIPE_DIR%/dmg_root</string>
				<key>dmg_path</key>
				<string>%RECIPE_DIR%/TheUnarchiver.dmg</string>
			</dict>
			<key>Processor</key>
			<string>DmgCreator</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>dmg_path</key>
				<string>%dmg_path%</string>
			</dict>
			<key>Processor</key>
			<string>AppDmgVersioner</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>source_path</key>
				<string>%dmg_path%</string>
				<key>destination_path</key>
				<string>%MUNKI_REPO%/pkgs/%NAME%-%version%.dmg</string>
			</dict>
			<key>Processor</key>
			<string>Copier</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>catalog</key>
				<string>%MUNKI_CATALOG%</string>
				<key>description</key>
				<string>%MUNKI_DESCRIPTION%</string>
				<key>displayname</key>
				<string>%MUNKI_DISPLAYNAME%</string>
				<key>info_path</key>
				<string>%MUNKI_REPO%/pkgsinfo/%NAME%-%version%.plist</string>
				<key>name</key>
				<string>%NAME%</string>
				<key>pkg_path</key>
				<string>%destination_path%</string>
			</dict>
			<key>Processor</key>
			<string>MunkiInfoCreator</string>
		</dict>
	</array>
</dict>
</plist>
